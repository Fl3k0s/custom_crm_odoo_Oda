<odoo>
    <data>
        <template id="report_visit_card">
          <t t-call="web.html_container">
              <t t-foreach="docs" t-as="o">
                  <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Visita</h2>
                        <p>Cif:
                            <span t-field="o.cif"/>
                        </p>
                        <p>Empresa:
                            <span t-field="o.empresa"/>
                        </p>
                        <p>Que vende:
                            <span t-field="o.queVende"/>
                        </p>
                        <p>Telefono Fijo:
                            <span t-field="o.fixedPhone"/>
                        </p>
                        <p>Telefono Movil:
                            <span t-field="o.phone"/>
                        </p>
                        <p>Email:
                            <span t-field="o.mail"/>
                        </p>
                        <p>Provincia:
                            <span t-field="o.provincia"/>
                        </p>
                    </div>
                  </t>
              </t>
          </t>
        </template>
    </data>
</odoo>